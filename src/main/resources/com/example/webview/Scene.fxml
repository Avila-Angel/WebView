<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.webview.Controller">
   <children>
      <WebView fx:id="webView" layoutX="-4.0" layoutY="59.0" prefHeight="342.0" prefWidth="600.0" />
      <Button layoutY="33.0" mnemonicParsing="false" onAction="#loadPage" prefHeight="26.0" prefWidth="43.0" text="load" />
      <TextField fx:id="textField" layoutY="8.0" onAction="#loadPage" prefHeight="26.0" prefWidth="600.0" />
      <Button layoutX="43.0" layoutY="33.0" mnemonicParsing="false" onAction="#refreshPage" prefHeight="26.0" prefWidth="58.0" text="refresh" />
      <Button layoutX="126.0" layoutY="33.0" mnemonicParsing="false" onAction="#zoomOut" text="-" />
      <Button layoutX="101.0" layoutY="33.0" mnemonicParsing="false" onAction="#zoomIn" text="+" />
      <Button layoutX="542.0" layoutY="33.0" mnemonicParsing="false" onAction="#displayHistory" text="History" />
      <Button layoutX="498.0" layoutY="33.0" mnemonicParsing="false" onAction="#back" text="&lt;" />
      <Button layoutX="520.0" layoutY="33.0" mnemonicParsing="false" onAction="#forward" text="&gt;" />
      <Button layoutX="149.0" layoutY="34.0" mnemonicParsing="false" onAction="#executeJS" text="execute JS" />
   </children>
</AnchorPane>
